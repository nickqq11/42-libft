NAME = libft.a

CC = gcc -Wall -Wextra -Werror 



OBJECTS = 

%.o : %.c 
	@$(CC) $< -o $@

.PHONY : clean fclean

clean :
	@rm -f *.o

fclean :
	@rm -f $(NAME)